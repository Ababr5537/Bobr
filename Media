class AudioPlayer:
    def play_audio(self):
        print("Відтворюється аудіо")

class VideoPlayer:
    def play_video(self):
        print("Відтворюється відео")

class MultimediaPlayer(AudioPlayer, VideoPlayer):
    def play(self):
        print("Відтворюється медіа")

if __name__ == "__main__":
    player = MultimediaPlayer()
    
    player.play_audio()
    player.play_video()
    player.play()
