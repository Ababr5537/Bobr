w = ["Баг", "Код", "Магія", "Слово", "Час", "Простір", "Матерія", "Енергія", "Відьма", "Вогонь", "Яблуко"]

try:
    index = int(input("Введіть номер скриньки (від 0 до 9): "))

    chw = w[index]
    print(f"Ви відкрили скриньку і знайшли: {chw}")

except IndexError:
    print("Помилка: Такої скриньки не існує dкажи число від 0 до 9.")

except ValueError:
    print("Помилка: Треба ввести число а не букву чи слово.")
